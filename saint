Handle = New("Part",Uzi,"Handle",{BrickColor = BrickColor.new("Black"),Transparency = 1,Transparency = 1,FormFactor = Enum.FormFactor.Custom,Size = Vector3.new(0.303939581, 0.303939581, 0.303939581),CFrame = CFrame.new(-63.9497833, 3.39759564, 8.0905323, -0.999997377, -1.38907907e-09, 5.60066837e-09, -4.67619166e-10, 0.999989152, 5.95959136e-07, -1.86810212e-09, 1.98648195e-07, -0.999986291),CanCollide = false,BottomSurface = Enum.SurfaceType.Smooth,TopSurface = Enum.SurfaceType.Smooth,Color = Color3.new(0.105882, 0.164706, 0.207843),})
