local function touch()
    if not using then
        using = true
        local Math = math.random(1,4)
        if Math == 1 then
            hit:Play()
        end
        if Math == 2 then
            hit1:Play()
        end
        if Math == 3 then
            hit2:Play()
        end
        if Math == 4 then
            hit3:Play()
        end
        wait(0.1)
        using = false
    end
end
sensoring.Touched:connect(touch)
sensoring1.Touched:connect(touch)
while true do
    clone.Head.CanCollide = false
    game:GetService("RunService").Stepped:wait()
end
end
 
