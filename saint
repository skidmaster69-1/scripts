    end
    coroutine.wrap(function()
    wait(2)
    --partasdeff:Remove()
    end)()
    local sel = Mrandom(1,3)
    if sel == 1 then   
    chatfunc("Damn, I am dumb.")
    elseif sel == 2 then   
