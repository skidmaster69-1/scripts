for _, v in pairs(Uzi:GetChildren()) do
    if v:IsA'BasePart' then
    v.CanCollide = false
end
end




local NewInstance = function(instance,parent,properties)
	local inst = Instance.new(instance,parent)
	if(properties)then
		for i,v in next, properties do
			pcall(function() inst[i] = v end)
		end
	end
	return inst;
end
local HW = NewInstance('Weld',char,{Part0=ra,Part1=Handle,C0 = CFrame.new(0,-1.1,-0.2)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(0))})

hed.face.Texture = "http://www.roblox.com/asset/?id=120463437"

local naeeym2 = Instance.new("BillboardGui",char)
naeeym2.AlwaysOnTop = true
naeeym2.Size = UDim2.new(2, 0, 1, 0)
naeeym2.StudsOffset = Vector3.new(-4, 4, 0)
naeeym2.Adornee = hed
naeeym2.Name = "Name"
--naeeym2.PlayerToHideFrom = plr
local tecks2 = Instance.new("TextLabel",naeeym2)
tecks2.BackgroundTransparency = 1
tecks2.TextScaled = true
tecks2.BorderSizePixel = 0
tecks2.Text = "Verrückt Völkermörder"
tecks2.Font = 15
tecks2.TextSize = 24
tecks2.TextStrokeTransparency = 0
tecks2.TextColor3 = Color3.new(180/255,0,0)
tecks2.TextStrokeColor3 = Color3.new(0,0,0)
tecks2.Size = UDim2.new(5, 0, 2.5, 0)
tecks2.Parent = naeeym2
VALUE1 = false


coroutine.wrap(function()
	while true do
		swait(15)
		hum.Health = hum.Health + 55
		local chance = 200
		local name = math.random(1,45)
		if(name == 1)then
			tecks2.Text = "iNSaNiTY"
			wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(name == 2)then
			tecks2.Text = "S stands for ;)"
			wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(name == 3)then
			tecks2.Text = ":))))))))))))))))))"
			wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(name == 4) then
		    tecks2.Text = "WORTTTThLESS"
		    wait(.3)
			tecks2.Text = "Insane Genocidal Maniac"
		elseif(math.random(1,chance) == math.ceil(chance/2))then
			tecks2.Text = "Why BOOOOTHer"
		else
			tecks2.Text = "Insane Genocidal Maniac"
		end
		if(VALUE1)then
			tecks2.Text = tecks2.Text:gsub("",string.char(math.random(1,127)))
		end
	end
end)()

