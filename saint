function Innocent_s_Tear()
	Attack = true
	for i = 0, 1, 0.17 do
		swait()
		PlayAnimationFromTable({
         CFrame.new(-0.0372427106, 0, -0.0254781954, 0.362814218, 0, -0.93186152, 0, 1, 0, 0.93186152, 0, 0.362814218) * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, 0), 
