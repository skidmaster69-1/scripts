for _, v in pairs(Doll:GetChildren()) do
    if v:IsA'BasePart' then
    v.CanCollide = false
	end
end

for _, v in pairs(Doll:GetChildren()) do
    if v:IsA'BasePart' then
    v.Transparency = 1
	end
end

